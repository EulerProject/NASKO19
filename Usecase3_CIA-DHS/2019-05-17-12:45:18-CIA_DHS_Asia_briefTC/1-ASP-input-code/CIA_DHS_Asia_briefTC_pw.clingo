ir(X, r14) :- out(cCIA_Asia,X), in(cDHS_Asia,X).
ir(X, r14) :- in(cCIA_Asia,X), out(cDHS_Asia,X).
ir(X, prod(r14,R)) :- out3(cCIA_Asia, X, R), in(cDHS_Asia,X), ix.
